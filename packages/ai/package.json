  "license": "MIT",
  "type": "module",
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@motif-ts/core": "workspace:*",
    "@motif-ts/middleware": "workspace:*",
    "@vitest/coverage-v8": "4.0.16",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "tsdown": "^0.18.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "@motif-ts/core": "workspace:*",
